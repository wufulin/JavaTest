<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wufulin.orm.entity">

    <class name="ShareGoodsLike" table="le_share_goods_like">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="userId" type="integer" column="userid"/>
        <property name="goodsId" type="integer" column="goods_id"/>
        <property name="commentType" type="short" column="commentType"/>
        <property name="addtime" type="long" column="addtime"/>
    </class>

</hibernate-mapping>
